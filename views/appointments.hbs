      </div>
      {{/if}}
      <div class="container mt-5">
        {{!-- provider_id, firstname, lastname, provider_specialty, email, phone_number, date_joined --}}

    
        <table class="table mt-5">
          <h2 style="margin-bottom: -40px; text-align: center; color:white;">Patients Appointment Data</h2>
          <thead>
            <tr class="theading">
              <th scope="col">appointment_id</th>
              <th scope="col">Patient Email</th>
              <th scope="col">Patient First_Name</th>
              <th scope="col">Doctor Id</th>
              <th scope="col">Doctor Email</th>
              <th scope="col">Doctor specialty</th>
              <th scope="col">Appointment_date</th>
              <th scope="col">status</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <tbody>
            {{#each rows}}
            <tr>
              {{!-- appointment_date, appointment_time, firstname, email, doctor_id, email, specialty --}}
              <th scope="row">{{this.appointment_id}}</th>
              <td>{{this.email}}</td>
              <td>{{this.firstname}}</td>
              <td>{{this.doctor_id}}</td>
              <td>{{this.doctoremail}}</td>
              <td>{{this.specialty}}</td>
              <td>{{this.appointment_date}}</td>
              <td>{{this.status}}</td>
              <td class="text-end">
                <a style="font-size: 13px;" href="/cancel/{{this.appointment_id}}" class="btn btn-light"><i class="bi bi-x-square-fill"></i>Cancel</a>
                <a style="font-size: 13px;" href="/editappointment/{{this.appointment_id}}" class="btn btn-light"><i class="bi bi-bookmarks"></i></i>Postponed</a>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>

